{
  "manifest_version": 3,
  "name": "rePort",
  "version": "1.1",
  "description": "Quick access to Amazon Seller Central reports",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://sellercentral.amazon.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
